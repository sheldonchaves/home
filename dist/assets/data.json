{
  "groups": [{
    "title": "Grep | find",
    "color": "#EDC951",
    "items": [{
      "name": "Recursively Search All Files For A String",
      "code": "grep -r 'string' ."
    },
    {
      "name": "file type",
      "code": "grep -r --include=*.txt 'val'"
    },
    {
      "name": "ignore case",
      "code": "grep -ri 'word' ."
    },
    {
      "name": "specify directory name",
      "code": "grep -r -l 'foo' /path/to/dir/*.java"
    },
    {
      "name": "with find",
      "code": "find /path/to/dir -type f -print0 | xargs -0 grep -l 'foo'"
    },
    {
      "name": "grep multiple2",
      "code": "grep -E 'word1|word2' *.doc"
    },
    {
      "name": "grep multiple ignore case and color recursively",
      "code": "grep -wir --color 'warning|error|critical' /var/log/"
    },
    {
      "name": "search in file",
      "code": "grep -qFx 'conteudo' 'arquivo.txt'"
    }
  ]
},
